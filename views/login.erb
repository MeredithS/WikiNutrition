<div id="login-page">
	<h1 id="login">Log In</h1>
	<div id="login-container">
		<div id="login-content">
			<form action="/sessions" method="POST">
			<label id="label-user"for="user">Username:</label>
			<input id="user" name="user_name" type="text">
			<label for="password">Password:</label>
			<input id="password" name="password" type="password">
			<input type="submit" value="Log In!">
			</form>
			<h3 id="account">If you don't have an account please <a href="/users/new">join us!</a></h3>
		</div><!--end div login-content-->
	</div><!--end div login-container-->
</div><!--end div login-page-->